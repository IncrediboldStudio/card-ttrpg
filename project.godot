; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="godot-starter-template"
run/main_scene="res://src/scenes/start_menu.tscn"
config/features=PackedStringArray("4.1", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

SceneLoader="*res://src/system/scene_loader.gd"
Panku="*res://addons/panku_console/console.tscn"
KeyPersistence="*res://src/system/key_persistence.gd"
SfxManager="*res://src/system/game_settings/audio/sfx_manager.gd"
MusicManager="*res://src/system/game_settings/audio/music_manager.gd"

[debug]

gdscript/warnings/unused_variable=2

[display]

window/size/viewport_width=1080
window/size/viewport_height=720
window/stretch/mode="viewport"
window/stretch/aspect="expand"

[editor_plugins]

enabled=PackedStringArray("res://addons/panku_console/plugin.cfg", "res://addons/theme_editor/plugin.cfg")

[gui]

theme/custom="res://src/ui/th_default.tres"

[input]

test_input={
"deadzone": 0.5,
"events": [Object(InputEventKey,"resource_local_to_scene":false,"resource_name":"","device":-1,"window_id":0,"alt_pressed":false,"shift_pressed":false,"ctrl_pressed":false,"meta_pressed":false,"pressed":false,"keycode":47,"physical_keycode":0,"key_label":0,"unicode":47,"echo":false,"script":null)
]
}

[panku]

disable_on_release=true
